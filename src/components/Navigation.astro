---
const links = [
	{ url: "/", title: "Home" },
	{ url: "/about", title: "about" },
	{ url: "/products", title: "products" },
];
---

<nav>
	<ul class="m-0 flex gap-2 h-fit bg-[#eee] mb-[20px] p-2">
		{
			links.map((link) => (
				<li>
					<a href={link.url}>{link.title}</a>
				</li>
			))
		}
	</ul>
</nav>
